#ifndef inlib_spark
#define inlib_spark

#include <vector>
#include <cstdio>

namespace inlib {
namespace spark {

inline char const* greet() {
  return "inlib/spark/greet : hello.";
}

inline void get_xyzs(const std::vector<double>& a_x,const std::vector<double>& a_y,const std::vector<double>& a_z) {
  ::printf("inlib::spark::get_xyzs : %lu, %lu, %lu\n",a_x.size(),a_y.size(),a_z.size());
  if(a_x.size() && a_y.size() && a_z.size()) {
    ::printf("last : %g, %g, %g\n",a_x[a_x.size()-1],a_y[a_y.size()-1],a_z[a_z.size()-1]);
  }
}

}}

#endif
